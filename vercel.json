{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.routes",
      "use": "index.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": ".(.*)",
      "dest": "public/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
